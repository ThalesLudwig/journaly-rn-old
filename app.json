{
  "expo": {
    "name": "Journaly",
    "slug": "Journaly",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5cb4d1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.valentini.journaly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#5cb4d1"
      },
      "package": "com.valentini.journaly"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
